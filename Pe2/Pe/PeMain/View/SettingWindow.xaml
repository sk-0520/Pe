<BaseWindow:CommonDataWindow 
	x:Class="ContentTypeTextNet.Pe.PeMain.View.SettingWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:BaseWindow="clr-namespace:ContentTypeTextNet.Pe.PeMain.View.Parts.WindowBase"
	xmlns:UI="clr-namespace:ContentTypeTextNet.Pe.PeMain.View.Parts"
	UI:XamlTag.LanguageKey="window/setting"
	Title="SettingWindow" 
	Height="261.576" 
	Width="527.586"
>
	<Window.Resources>
		<ResourceDictionary Source="/View/Parts/Resource/DialogDictionary.xaml"/>
	</Window.Resources>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="auto"/>
		</Grid.RowDefinitions>
		<Grid Style="{StaticResource ResourceKey=DialogResultParent}" Grid.Row="1">
			<StackPanel Style="{StaticResource ResourceKey=DialogResultPanel}" >
				<Button Style="{StaticResource ResourceKey=DialogResultButtonAccept}" />
				<Button Style="{StaticResource ResourceKey=DialogResultButtonCancel}" />
			</StackPanel>
		</Grid>

	</Grid>
</BaseWindow:CommonDataWindow>
