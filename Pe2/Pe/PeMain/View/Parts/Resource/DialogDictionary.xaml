<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:UI="clr-namespace:ContentTypeTextNet.Pe.PeMain.View.Parts"
>
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/View/Parts/Resource/XamlTagDictionary.xaml" />
		<ResourceDictionary>
			<Style x:Key="DialogResultPanel" TargetType="StackPanel">
				<Setter Property="VerticalAlignment" Value="Center" />
				<Setter Property="HorizontalAlignment" Value="Right" />
				<Setter Property="Margin" Value="0, 4, 0, 4" />
				<Setter Property="Orientation" Value="Horizontal" />
			</Style>
			<Style x:Key="DialogResultButton" TargetType="Button" BasedOn="{StaticResource ResourceKey=LanguageKeyButton}">
				<Setter Property="Width" Value="120" />
				<Setter Property="Height" Value="28" />
				<Setter Property="Margin" Value="0, 0, 10, 0" />
			</Style>
			<Style x:Key="DialogResultButtonAccept" TargetType="Button" BasedOn="{StaticResource ResourceKey=DialogResultButton}">
				<Setter Property="UI:XamlTag.LanguageKey" Value="common/ok" />
				<Setter Property="IsDefault" Value="True" />
			</Style>
			<Style x:Key="DialogResultButtonCancel" TargetType="Button" BasedOn="{StaticResource ResourceKey=DialogResultButton}">
				<Setter Property="UI:XamlTag.LanguageKey" Value="common/cancel" />
			</Style>
		</ResourceDictionary>
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>