{
	"name": "pe-documents",
	"version": "1.0.0",
	"description": "Pe documents",
	"main": "index.pug",
	"scripts": {
		"lint": "eslint Source/Documents/source/script",
		"document-watch": "parcel watch --no-autoinstall --out-dir Source/Documents/build/ --public-url . Source/Documents/source/index.pug",
		"document-build:clean": "rimraf Source/Documents/build/*",
		"document-build:output": "parcel build --no-autoinstall  --no-source-maps --out-dir Source/Documents/build/ --no-content-hash --public-url . Source/Documents/source/index.pug",
		"document-build": "npm run document-build:clean & npm run document-build:output",
		"check": "npm outdated"
	},
	"author": "sk",
	"license": "WTFPL",
	"repository": {
		"type": "git",
		"url": "git@bitbucket.org:sk_0520/pe.git"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"eslint": "^7.8.1",
		"parcel-bundler": "^1.12.4",
		"pug": "^3.0.0",
		"rimraf": "^3.0.2",
		"sass": "^1.26.10",
		"typescript": "^3.9.7"
	}
}
