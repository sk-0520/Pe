<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:sys="clr-namespace:System;assembly=mscorlib"
	xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
>
	<Style x:Key="TypeGridStyle" TargetType="Grid">
	</Style>
	<Style x:Key="TypeColumnSlider" TargetType="ColumnDefinition">
		<Setter Property="Width" Value="6*" />
	</Style>
	<Style x:Key="TypeColumnUpDown" TargetType="ColumnDefinition">
		<Setter Property="Width" Value="4*" />
	</Style>
	<Style x:Key="TypeSliderStyle" TargetType="Slider">
		<Setter Property="Grid.Column" Value="0" />
	</Style>
	<Style x:Key="TypeValueStyle" TargetType="TextBox">
		<Setter Property="Grid.Column" Value="1" />
	</Style>
</ResourceDictionary>