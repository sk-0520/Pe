<Window
  x:Class="ContentTypeTextNet.Pe.Main.View.Accept.AcceptWindow"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

  Title="AcceptWindow"
  Height="450"
  Width="800"

  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
>
  <Grid>
    <StackPanel>
      <Button Command="{Binding AffirmativeCommand}">OK</Button>
      <Button Command="{Binding NegativeCommand}">Cancel</Button>
    </StackPanel>
  </Grid>
</Window>
