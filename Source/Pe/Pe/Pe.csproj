<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <Platforms>x64;x86</Platforms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <RootNamespace>ContentTypeTextNet.Pe.Main</RootNamespace>
  </PropertyGroup>


  <ItemGroup>
    <None Remove="nlog.config" />
    <None Remove="Resources\Icon\App.ico" />
    <None Remove="Resources\Icon\NotifyIcon\App-beta.ico" />
    <None Remove="Resources\Icon\NotifyIcon\App-debug.ico" />
    <None Remove="Resources\Icon\NotifyIcon\App-release.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="etc\nlog.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="2.2.0" />
    <PackageReference Include="NLog.Extensions.Logging" Version="1.5.4" />
    <PackageReference Include="Prism.Wpf" Version="7.2.0.1367" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Pe.Core\Pe.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="doc\help\" />
    <Folder Include="doc\license\" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Icon\App.ico" />
    <Resource Include="Resources\Icon\NotifyIcon\App-beta.ico" />
    <Resource Include="Resources\Icon\NotifyIcon\App-debug.ico" />
    <Resource Include="Resources\Icon\NotifyIcon\App-release.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="etc\sql\ContentTypeTextNet.Pe.Main\**\*.sql">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Update="C:\Users\abcde\.nuget\packages\microsoft.ui.xaml\2.2.190917002\buildTransitive\DensityStyles\Compact.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>

</Project>