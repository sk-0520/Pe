<Window
  x:Class="ContentTypeTextNet.Pe.Main.View.Startup.StartupWindow"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

  xmlns:app="clr-namespace:ContentTypeTextNet.Pe.Main"

  Title="StartupWindow"
  Width="800"
  Height="450"

  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
>
  <Grid>
    <StackPanel>
      <Button Command="{Binding ImportProgramsCommand}">import programs</Button>
      <Button Command="{Binding RegisterStartupCommand}">register startup</Button>
      <Button Command="{Binding ShowNotificationAreaCommand}">notify area</Button>
    </StackPanel>
  </Grid>
</Window>
