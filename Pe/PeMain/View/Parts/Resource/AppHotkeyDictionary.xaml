<ResourceDictionary 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:app_main_view_parts_control="clr-namespace:ContentTypeTextNet.Pe.PeMain.View.Parts.Control"
  x:Class="uint"
>
  <ResourceDictionary.MergedDictionaries>
    <ResourceDictionary Source="/View/Parts/Resource/TextBoxDictionary.xaml"/>
  </ResourceDictionary.MergedDictionaries>
  <Style x:Key="DefaultAppHotkey" TargetType="app_main_view_parts_control:AppHotkeyControl">
    <Setter Property="MinWidth" Value="140" />
    <Setter Property="Padding" Value="{StaticResource TextBoxPadding}" />
    <Setter Property="VerticalAlignment" Value="Center" />
  </Style>
</ResourceDictionary>