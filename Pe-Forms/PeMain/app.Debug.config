<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="page-update" value="http://localhost/update.xml?{TIMESTAMP}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="page-changelog-release" value="http://localhost/update-release.html?{TIMESTAMP}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="page-changelog-rc" value="http://localhost/update-rc.html?{TIMESTAMP}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	</appSettings>
</configuration>
